{
  "V1TransmitFetcher": {
    "forks": 1,
    "script": "services/transmitFetcher.js",
    "namespace": "LiqRegistry",
    "listen": {
      "transmit": {
        "topic": "data/eth/pending/transmit_assets",
        "roundrobing": true
      },
      "onReservesData": {
        "topic": "data:reserves:V1",
        "roundrobing": false
      }
    },
    "notify": {
      "delete": {
        "topic": "logger/liquidator/V1"
      },
      "reject": {
        "topic": "logger/liquidator/V1"
      },
      "errorMessage": {
        "topic": "logger/liquidator/V1"
      },
      "start": {
        "topic": "logger/liquidator/V1"
      },
      "stop": {
        "topic": "logger/liquidator/V1"
      },
      "liquidateEvent": {
        "topic": "logger/liquidator/V1"
      },
      "liquidateCommand": {
        "topic": "execute/liquidator/V1"
      },
      "info": {
        "topic": "logger/liquidator/V1"
      }
    },
    "params": {
      "protocol": "V1",
      "settings": {
        "minHfDel": "0.2",
        "maxHfDel": "1.1",
        "minHfLiq": "0.9",
        "maxHfLiq": "1",
        "minBorrow": "0.1",
        "minCollateral": "0.1"
      },
      "configPath": "/configs/Main.json"
    }
  },
