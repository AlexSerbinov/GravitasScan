{
  "DefaultSettings": {
    "forks": 1,
    "script": "services/archive.js",
    "namespace": "Archive",
    "listen": {
      "onBlock": {
        "topic": "data:block",
        "roundrobing": false
      }
    },
    "notify": {
      "start": {
        "topic": "logger/liquidator"
      },
      "info": {
        "topic": "logger/liquidator"
      },
      "stop": {
        "topic": "logger/liquidator"
      }
    },
    "params": {
      "service": "archive",
      "settings": {
        "mode": "node"
      },
      "configPath": "/configs/Main.json"
    }
  },

  "V1Archive": {
    "params": {
      "protocol": "V1"
    }
  },

  "V2Archive": {
    "params": {
      "protocol": "V2"
    }
  },

  "V3Archive": {
    "params": {
      "protocol": "V3"
    }
  },

  "CompoundArchive": {
    "params": {
      "protocol": "Compound"
    }
  }
}
