{
  "V1Proxy": {
    "forks": 1,
    "script": "services/proxy.js",
    "namespace": "namespace",
    "listen": {
      "drain": {
        "topic": "event:drain:subgraph:V1",
        "roundrobing": false
      }
    },
    "notify": {
      "sendUsersToSubgraph": {
        "topic": "event:users:subraph:V1"
      },
      "errorMessage": {
        "topic": "event:error:proxy:V1"
      },
      "start": {
        "topic": "event:start:proxy:V1"
      },
      "info": {
        "topic": "event:info:proxy:V1"
      }
    },
    "params": {
      "PROTOCOL": "V1",
      "configPath": "/configs/Main.json"
    }
  },
