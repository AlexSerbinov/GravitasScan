{
  "V1Archive": {
    "forks": 1,
    "script": "services/archive.js",
    "namespace": "Archive",
    "listen": {
      "onBlock": {
        "topic": "data:block",
        "roundrobing": false
      }
    },

    "notify": {
      "sendFetchedUsersEvent": {
        "topic": "logger/liquidator"
      },
      "start": {
        "topic": "logger/liquidator"
      },
      "info": {
        "topic": "logger/liquidator"
      }
    },
    "params": {
      "service": "archive",
      "protocol": "V1",
      "settings": {
        "mode": "node"
      },
      "configPath": "/configs/Main.json"
    }
  },
