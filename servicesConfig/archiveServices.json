{
  "V1Archive": {
    "forks": 1,
    "script": "services/archive.js",
    "namespace": "namespace",
    "listen": {
      "onBlock": {
        "topic": "data:block",
        "roundrobing": false
      }
    },

    "notify": {
      "sendFetchedUsersEvent": {
        "topic": "event:archive_users:archive:V1"
      },
      "sendMonitoringUsersEvent": {
        "topic": "event:listener_users:archive:V1"
      },

      "start": {
        "topic": "event:start:archive:V1"
      },
      "info": {
        "topic": "event:info:archive:V1"
      }
    },
    "params": {
      "protocol": "V1",
      "configPath": "/configs/Main.json"
    }
  },
