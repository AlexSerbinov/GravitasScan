{
  "DefaultSetings": {
    "forks": 1,
    "script": "services/events.js",
    "namespace": "Events",
    "notify": {
      "sendBlock": {
        "topic": "data:block"
      },
      "start": {
        "topic": "logger/liquidator"
      },
      "info": {
        "topic": "logger/liquidator"
      },
      "errorMessage": {
        "topic": "logger/liquidator"
      }
    }
  },

  "V1Events": {
    "notify": {
      "sendGlobalReserves": {
        "topic": "data:reserves:V1"
      }
    },
    "params": {
      "protocol": "V1"
    }
  },

  "V2Events": {
    "notify": {
      "sendGlobalReserves": {
        "topic": "data:reserves:V2"
      }
    },
    "params": {
      "protocol": "V2"
    }
  },

  "V3Events": {
    "notify": {
      "sendGlobalReserves": {
        "topic": "data:reserves:V3"
      }
    },
    "params": {
      "protocol": "V3"
    }
  },

  "CompoundEvents": {
    "notify": {
      "sendGlobalReserves": {
        "topic": "data:reserves:Compound"
      }
    },
    "params": {
      "protocol": "Compound"
    }
  }
}
