{
  "DefaultSettings": {
    "forks": 1,
    "script": "services/proxy.js",
    "namespace": "Proxy",
    "notify": {
      "errorMessage": {
        "topic": "logger/liquidator"
      },
      "start": {
        "topic": "logger/liquidator"
      },
      "info": {
        "topic": "logger/liquidator"
      }
    },
    "params": {
      "service": "proxy",
      "batchSize": 35,
      "SEND_WITHOUT_DRAIN_TIMEOUT": 3600000,
      "configPath": "/configs/Main.json"
    }
  },
  "V1Proxy": {
    "listen": {
      "drain": {
        "topic": "event/drain/subgraph/V1",
        "roundrobin": false
      }
    },
    "notify": {
      "sendUsersToSubgraph": {
        "topic": "event/users/proxy/V1"
      }
    },
    "params": {
      "protocol": "V1"
    }
  },
  "V2Proxy": {
    "listen": {
      "drain": {
        "topic": "event/drain/subgraph/V2",
        "roundrobin": false
      }
    },
    "notify": {
      "sendUsersToSubgraph": {
        "topic": "event/users/proxy/V2"
      }
    },
    "params": {
      "protocol": "V2"
    }
  },
  "V3Proxy": {
    "listen": {
      "drain": {
        "topic": "event/drain/subgraph/V3",
        "roundrobin": false
      }
    },
    "notify": {
      "sendUsersToSubgraph": {
        "topic": "event/users/proxy/V3"
      }
    },
    "params": {
      "protocol": "V3"
    }
  },
  "CompoundProxy": {
    "listen": {
      "drain": {
        "topic": "event/drain/subgraph/Compound",
        "roundrobin": false
      }
    },
    "notify": {
      "sendUsersToSubgraph": {
        "topic": "event/users/proxy/Compound"
      }
    },
    "params": {
      "protocol": "Compound"
    }
  }
}
