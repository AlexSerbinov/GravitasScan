{
  "DefaultSettings": {
    "forks": 1,
    "script": "services/transmitFetcher.js",
    "namespace": "transmit",
    "listen": {
      "transmit": {
        "topic": "mempool/transmit",
        "roundrobing": true
      }
    },
    "notify": {
      "errorMessage": {
        "topic": "logger/liquidator"
      },
      "start": {
        "topic": "logger/liquidator"
      },
      "stop": {
        "topic": "logger/liquidator"
      },
      "liquidateEvent": {
        "topic": "logger/liquidator"
      },
      "info": {
        "topic": "logger/liquidator"
      }
    },
    "params": {
      "filters": {
        "minHfDel": "0.8",
        "maxHfDel": "1.15",
        "minHfLiq": "0.9",
        "maxHfLiq": "1",
        "minBorrow": "0.02",
        "minCollateral": "0.02"
      },
      "maxNumberOfUsersToSimulate": "30",
      "service": "transmitFetcher",
      "configPath": "/configs/Main.json",
      "enso_url": "http://10.10.100.82:9992/api/v1/simulate-bundle"
    }
  },

  "V1TransmitFetcher": {
    "listen": {
      "onReservesData": {
        "topic": "data/reserves/V1",
        "roundrobing": false
      }
    },
    "notify": {
      "liquidateCommand": {
        "topic": "execute/liquidator/V1"
      }
    },
    "params": {
      "protocol": "V1"
    }
  },

  "V2TransmitFetcher": {
    "listen": {
      "onReservesData": {
        "topic": "data/reserves/V2",
        "roundrobing": false
      }
    },
    "notify": {
      "liquidateCommand": {
        "topic": "execute/liquidator/V2"
      }
    },
    "params": {
      "protocol": "V2"
    }
  },

  "V3TransmitFetcher": {
    "listen": {
      "onReservesData": {
        "topic": "data/reserves/V3",
        "roundrobing": false
      }
    },
    "notify": {
      "liquidateCommand": {
        "topic": "execute/liquidator/V3"
      }
    },
    "params": {
      "protocol": "V3"
    }
  },

  "CompoundTransmitFetcher": {
    "listen": {
      "onReservesData": {
        "topic": "data/reserves/Compound",
        "roundrobing": false
      }
    },
    "notify": {
      "liquidateCommand": {
        "topic": "execute/liquidator/Compound"
      }
    },
    "params": {
      "protocol": "Compound"
    }
  }
}
