{
  "semi": false,
  "singleQuote": false,
  "printWidth": 320,
  "tabWidth": 2,
  "useTabs": false,
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "jsxBracketSameLine": false,
  "objectPropertyNewline": {
    "allowAllPropertiesOnSameLine": false,
    "maxPropertiesPerLine": 1
  }
}
