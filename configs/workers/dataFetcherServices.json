{
  "V1DataFetcher": {
    "forks": 1,
    "script": "services/dataFetcher.js",
    "namespace": "DataFetcher",
    "listen": {
      "searcherExecute": {
        "topic": "execute/dataFetcher/V1",
        "roundrobing": true
      },
      "onReservesData": {
        "topic": "data:reserves:V1",
        "roundrobing": false
      }
    },
    "notify": {
      "pushToRedis": {
        "topic": "logger/liquidator"
      },
      "deleteFromRedis": {
        "topic": "logger/liquidator"
      },
      "reject": {
        "topic": "logger/liquidator"
      },
      "errorMessage": {
        "topic": "logger/liquidator"
      },
      "start": {
        "topic": "logger/liquidator"
      },
      "stop": {
        "topic": "logger/liquidator"
      },
      "liquidateEvent": {
        "topic": "logger/liquidator"
      },
      "liquidateCommand": {
        "topic": "execute/liquidator/V1"
      },
      "info": {
        "topic": "logger/liquidator"
      }
    },
    "params": {
      "service": "dataFetcher",
      "protocol":  "V1",
      "settings": {
        "minHfDel": "0.2",
        "maxHfDel": "1.5",
        "minHfLiq": "0.9",
        "maxHfLiq": "1",
        "minBorrow": "0.05",
        "minCollateral": "0.05"
      },
      "configPath": "/configs/Main.json"
    }
  },
