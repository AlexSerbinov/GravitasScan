{
  "V1Proxy": {
    "forks": 1,
    "script": "services/proxy.js",
    "namespace": "LiqRegistry",
    "listen": {
      "drain": {
        "topic": "event/drain/subgraph/V1",
        "roundrobing": false
      }
    },
    "notify": {
      "sendUsersToSubgraph": {
        "topic": "event/users/proxy/V1"
      },
      "errorMessage": {
        "topic": "logger/liquidator/V1"
      },
      "start": {
        "topic": "logger/liquidator/V1"
      },
      "info": {
        "topic": "logger/liquidator/V1"
      }
    },
    "params": {
      "service": "proxy",
      "protocol":  "V1",
      "batchSize": 20,
      "SEND_WITHOUT_DRAIN_TIMEOUT": 3600000,
      "configPath": "/configs/Main.json"
    }
  },
