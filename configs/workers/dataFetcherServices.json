{
  "DefaultSettings": {
    "forks": 1,
    "script": "services/dataFetcher.js",
    "namespace": "DataFetcher",
    "notify": {
      "errorMessage": {
        "topic": "logger/liquidator"
      },
      "start": {
        "topic": "logger/liquidator"
      },
      "info": {
        "topic": "logger/liquidator"
      },
      "stop": {
        "topic": "logger/liquidator"
      }
    },
    "params": {
      "service": "dataFetcher",
      "filters": {
        "minHfDel": "0.2",
        "maxHfDel": "1.1",
        "minHfLiq": "0.9",
        "maxHfLiq": "1",
        "minBorrow": "0.1",
        "minCollateral": "0.1"
      },
      "configPath": "/configs/Main.json"
    }
  },

  "V1DataFetcher": {
    "listen": {
      "searcherExecute": {
        "topic": "execute/dataFetcher/V1",
        "roundrobing": true
      },
      "onReservesData": {
        "topic": "data:reserves:V1",
        "roundrobing": false
      }
    },
    "notify": {
      "liquidateCommand": {
        "topic": "execute/liquidator/V1"
      }
    },
    "params": {
      "protocol": "V1"
    }
  },

  "V2DataFetcher": {
    "listen": {
      "searcherExecute": {
        "topic": "execute/dataFetcher/V2",
        "roundrobing": true
      },
      "onReservesData": {
        "topic": "data:reserves:V2",
        "roundrobing": false
      }
    },
    "notify": {
      "liquidateCommand": {
        "topic": "execute/liquidator/V2"
      }
    },
    "params": {
      "protocol": "V2"
    }
  },

  "V3DataFetcher": {
    "listen": {
      "searcherExecute": {
        "topic": "execute/dataFetcher/V3",
        "roundrobing": true
      },
      "onReservesData": {
        "topic": "data:reserves:V3",
        "roundrobing": false
      }
    },
    "notify": {
      "liquidateCommand": {
        "topic": "execute/liquidator/V3"
      }
    },
    "params": {
      "protocol": "V3"
    }
  },

  "CompoundDataFetcher": {
    "listen": {
      "searcherExecute": {
        "topic": "execute/dataFetcher/Compound",
        "roundrobing": true
      },
      "onReservesData": {
        "topic": "data:reserves:Compound",
        "roundrobing": false
      }
    },
    "notify": {
      "liquidateCommand": {
        "topic": "execute/liquidator/Compound"
      }
    },
    "params": {
      "protocol": "Compound"
    }
  }
}
